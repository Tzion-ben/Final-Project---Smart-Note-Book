<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ArielUniversity.finalproject">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:name= "com.ArielUniversity.finalproject.AccessDB.DBConnect"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalProject">
        <activity android:name="com.ArielUniversity.finalproject.View.EntryActivities.RegisterActivity"></activity>
        <activity android:name="com.ArielUniversity.finalproject.View.EntryActivities.LoginActivity" />
        <activity android:name="com.ArielUniversity.finalproject.View.UserActivity" />
        <activity android:name="com.ArielUniversity.finalproject.Models.Activation_Model.SensorModel" />
        <activity android:name="com.ArielUniversity.finalproject.View.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>